colors:
  name: aikofog
  author: Gutterslob
  primary:
    background: "#f1eee9"
    foreground: "#b2b3b8"
  cursor:
    text: "#f1eee9"
    cursor: "#b2b3b8"
  normal:
    black: "#757475"
    red: "#936a6f"
    green: "#668462"
    yellow: "#7f7b62"
    blue: "#6e7291"
    magenta: "#846887"
    cyan: "#637e7b"
    white: "#9a9a9a"
  bright:
    black: "#4a4a4a"
    red: "#d76572"
    green: "#4fae42"
    yellow: "#a8981f"
    blue: "#7782cf"
    magenta: "#b754c4"
    cyan: "#2ea89a"
    white: "#c4c4c4"
key_bindings:
  - {
      key: Return,
      mods: Control|Shift,
      action: SpawnNewInstance
    }

window.opacity: 1.00

window:
  padding:
    x: 10
    y: 10

font:
  normal:
    family: Mononoki Nerd Font Mono
    style: regualr
  size: 12
live_config_reload: true
dynamic_padding: true
theme: Gutterslob - aikofog
